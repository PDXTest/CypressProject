{
  "name": "cypressproject",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "pretest": "rimraf -r ./cypress/report/*.json && rimraf -r ./cypress/report/*.html",
    "test": "npm run cypress-test || npm run posttest",
    "cypress-test": "cypress run",
    "posttest": "npm run merge-reports && npm run generate-htmlreport",
    "merge-reports": "mochawesome-merge ./cypress/report/*.json > ./cypress/report/jsonFinalReport.json",
    "generate-htmlreport": "marge ./cypress/report/jsonFinalReport.json --reportDir ./cypress/report/FinalHTMLreport.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^6.3.0",
    "cypress-mochawesome-reporter": "^3.2.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "rimraf": "^3.0.2"
  }
}
